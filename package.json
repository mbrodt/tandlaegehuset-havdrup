{
  "name": "gatsby-starter-default",
  "description": "Gatsby default starter",
  "version": "1.0.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "aos": "^2.3.4",
    "autoprefixer": "^9.3.1",
    "contentful": "^7.0.5",
    "dotenv": "^6.1.0",
    "gatsby": "^2.0.50",
    "gatsby-background-image": "^0.7.4",
    "gatsby-image": "^2.0.15",
    "gatsby-plugin-manifest": "^2.0.5",
    "gatsby-plugin-offline": "^2.0.11",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-sharp": "^2.0.17",
    "gatsby-source-contentful": "^2.0.14",
    "gatsby-source-filesystem": "^2.0.4",
    "gatsby-transformer-sharp": "^2.1.10",
    "google-map-react": "^1.1.1",
    "npm-run-all": "^4.1.3",
    "postcss-cli": "^6.0.1",
    "react": "^16.8.1",
    "react-anchor-link-smooth-scroll": "^1.0.12",
    "react-dom": "^16.8.1",
    "react-helmet": "^5.2.0",
    "react-spring": "^7.2.1",
    "run-p": "0.0.0",
    "tailwindcss": "^0.7.2"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build:css": "postcss src/css/index.css -o src/css/generated.css",
    "watch:css": "postcss src/css/index.css -o src/css/generated.css -w",
    "build": "npm run build:css && gatsby build",
    "dev": "run-p watch:css gatsby",
    "gatsby": "gatsby develop",
    "format": "prettier --write \"src/**/*.js\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "prettier": "^1.14.2",
    "tailwindcss-plugins": "^0.1.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  }
}
